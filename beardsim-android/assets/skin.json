{
	com.badlogic.gdx.graphics.g2d.BitmapFont: {
		default-font: { file: coders-crux.fnt },
		bold-font: { file: coders-crux.fnt }
	},
	
	com.badlogic.gdx.graphics.Color: {
		white: { r: 1, g: 1, b: 1, a: 1 },
		black: { r: 0, g: 0, b: 0, a: 1 },
		red: { r: 1, g: 0, b: 0, a: 1 }
	},

	com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
		default: {
			font: default-font,
			fontColor: black
		},
		title: {
			font: bold-font,
			fontColor: black
		}
	},
	com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
		default: {
			up: button,
			down: button,
			checked: button
		},
		toparrow: {
			up: downblock
		},
		leftarrow: {
			up: rightblock
		},
		rightarrow: {
			up: leftblock
		},
		bottomarrow: {
			up: upblock
		},
	},
	com.badlogic.gdx.scenes.scene2d.ui.ImageButton$ImageButtonStyle: {
		default: {
			up: button,
			down: button,
			checked: button
		}
	},
	com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
		default: {
			font: default-font,
			fontColor: black,
			up: button,
			down: button,
			checked: button
		},
		square: {
			font: default-font,
			fontColor: black,
			up: block,
			down: block,
			checked: block
		}
	},
	com.badlogic.gdx.scenes.scene2d.ui.Slider$SliderStyle: {
		default-horizontal: {
			background: button,
			knob: sliderdot
		},
		default-vertical: {
			background: button,
			knob: sliderdot
		}
	},
	com.badlogic.gdx.scenes.scene2d.ui.Window$WindowStyle: {
		default: {
			titleFont: bold-font,
			titleFontColor: black,
			stageBackground: whitebg,
			background: button
		}
	}
}